{
    "name": "kowap/laravel-sendpulse",
    "description": "Laravel package for SendPulse REST API",
    "keywords": ["laravel", "sendpulse", "email", "api"],
    "homepage": "https://github.com/<ваш-юзернейм>/laravel-sendpulse",
    "support": {
        "issues": "https://github.com/kowap/laravel-sendpulse/issues",
        "source": "https://github.com/kowap/laravel-sendpulse"
    },
    "type": "laravel-package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kowap\\SendPulse\\": "src/"
        }
    },
    "authors": [
        {
            "name": "ilya biloborodov",
            "email": "biloborodov.ilya@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "sendpulse/rest-api": "^1.0"
    },
    "require-dev": {
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "roave/security-advisories": "dev-latest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kowap\\SendPulse\\SendPulseServiceProvider"
            ],
            "aliases": {
                "SendPulse": "Kowap\\SendPulse\\SendPulseFacade"
            }
        }
    }
}
